
model {
  
  for (i in 1:N) {
      Y[i] ~ dnorm(mu[i], tau)T(0,)
      mu.temp[i] <- c*T[i]*(T[i]-T0)*sqrt((Tm-T[i])*(Tm>T[i]))
      mu[i] <- 0*(mu.temp[i]<0) + mu.temp[i]*(mu.temp[i]>0)
      
  }

  c ~ dgamma(1,10)
  Tm ~ dunif(25,45)
  T0  ~ dunif(0, 24)
  sigma<-1/tau
  tau ~ dgamma(0.0001, 0.0001)

}
